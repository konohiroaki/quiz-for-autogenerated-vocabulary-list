{
  "manifest_version": 2,
  "name": "Quiz for Autogenerated Vocabulary List",
  "version": "2.1.0",
  "description": "This is an extension to improve vocabulary by answering quiz from a vocabulary list generated based on your search history",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "alarms",
    "notifications",
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "kotlin.js",
      "kotlinx-coroutines-core.js",
      "shared.js",
      "background.js"
    ]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "https://translate.google.com/*",
        "https://eow.alc.co.jp/search?q=*",
        "https://ejje.weblio.jp/content/*"
      ],
      "js": [
        "kotlin.js",
        "shared.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  }
}
